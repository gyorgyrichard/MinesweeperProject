<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.layout.ColumnConstraints?>
<VBox prefWidth="600" prefHeight="480" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PrimaryController">

   <children>

      <Label text="Minesweeper"  />
      <Button fx:id="Start" text="Start Game" onAction="#switchToSecondary" alignment="CENTER" prefWidth="100" prefHeight="50"/>
      <Button fx:id="Options" text="Optons" onAction="#Options" alignment="CENTER" prefWidth="100" prefHeight="50"/>
      <Button fx:id="Exit" text="Exit" onAction="#Exit" alignment="CENTER" prefWidth="100" prefHeight="50"/>
      <Button fx:id="HighScore" text="HighScore" onAction="#HighScore" alignment="CENTER" prefWidth="100" prefHeight="50"/>

   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
